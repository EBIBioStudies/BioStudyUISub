<script type="text/ng-template" id="myModalWindow.html">
    <div tabindex="-1" role="dialog" class="modal fade" ng-class="{in: animate}" ng-style="{'z-index': 1050 + index*10, display: 'block'}" ng-click="close($event)">
        <div class="modal-dialog" ng-class="{'modal-sm': size == 'sm', 'modal-lg': size == 'lg'}">
            <div class="modal-content" modal-transclude>

            </div>
        </div>
    </div>
</script>

<script type="text/ng-template" id="myModalContentError.html">
    <alert class="margin-none" type="danger" close="$close();">
        <span ng-repeat="message in messages">
            {{message.msg}}
        </span>
    </alert>
</script>

<script type="text/ng-template" id="myModalContentSuccess.html">
    <alert class="margin-none" type="danger" close="$close();">
        <span ng-repeat="message in messages">
            {{message.msg}}
        </span>
    </alert>
</script>


