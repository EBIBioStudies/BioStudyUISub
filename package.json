{
  "name": "lib",
  "description": "BioStudies Submission Tool (UI)",
  "version": "0.0.0",
  "private": true,
  "main": "server/app.js",
  "scripts": {
    "build": "sh ./tasks/build.sh",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "node:beta": "cross-env NODE_ENV=beta node .",
    "node:dev": "cross-env NODE_ENV=development nodemon .",
    "node:prod": "cross-env NODE_ENV=production node .",
    "start-no-ssl": "ng serve --proxy-config proxy.conf.json --progress",
    "start:dev": "run-p node:dev start-no-ssl",
    "pretest": "npm run lint",
    "test": "jest --ci"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "clearMocks": true,
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts",
      "<rootDir>/src/setup-jest-after-env.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 21.44,
        "branches": 21.57,
        "functions": 18.81,
        "lines": 21.13
      }
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.extensions.ts",
      "!**/*.config.ts",
      "!**/*.module.ts",
      "!**/*.d.ts",
      "!**/*Mocks.ts",
      "!**/polyfills.ts",
      "!**/main.ts",
      "!**/index.ts"
    ],
    "coveragePathIgnorePatterns": [
      "src/environments"
    ]
  },
  "dependencies": {
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/router": "^6.0.7",
    "@fortawesome/fontawesome-free": "^5.4.2",
    "@rxweb/reactive-form-validators": "^1.9.1",
    "ag-grid-angular": "^20.0.0",
    "ag-grid-community": "^20.0.0",
    "core-js": "^2.4.1",
    "fp-ts": "^1.9.0",
    "helmet": "^3.21.1",
    "http-status-codes": "^1.3.2",
    "lodash": "^4.17.15",
    "ng-recaptcha": "^4.0.0-beta.1",
    "ng2-cookies": "^1.0.2",
    "ngx-bootstrap": "^3.1.4",
    "ngx-markdown": "^8.1.0",
    "pluralize": "^7.0.0",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.0.0-rc.0",
    "zone.js": "~0.8.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@testing-library/angular": "^8.0.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@types/jest": "^24.0.23",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^8.0.7",
    "@types/pluralize": "0.0.28",
    "body-parser": "^1.19.0",
    "codelyzer": "^4.4.2",
    "compression": "^1.7.4",
    "config": "^3.1.0",
    "cross-env": "^5.2.1",
    "del": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.5.1",
    "express-winston": "^3.1.0",
    "forever": "^1.0.0",
    "gulp": "^4.0.0",
    "gulp-zip": "^3.2.0",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "protractor": "^6.0.0",
    "rxjs-tslint-rules": "^4.23.1",
    "ts-node": "~2.0.0",
    "tslint": "^5.5.0",
    "typescript": "3.1.1",
    "winston": "^3.2.1"
  }
}
